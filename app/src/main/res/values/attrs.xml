<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CountDownView">
        <attr name="background_color" format="color" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="text" format="string" />
        <attr name="text_size" format="dimension" />
        <attr name="text_color" format="color" />
    </declare-styleable>



    <declare-styleable name="ColorTrackTabViewIndicator">
        <attr name="ctTabSelectedTextColor" format="color"/>
        <attr name="ctTabIndicatorColor" format="color"/>
        <attr name="ctTabTextColor" format="color"/>
        <attr name="ctTabTextSize" format="dimension"/>
        <attr name="ctTabWidth" format="dimension"/>
        <attr name="ctMode">
            <enum name="scrollable" value="0"/>
            <enum name="fixed" value="1"/>
        </attr>

    </declare-styleable>

    <declare-styleable name="ColorTrackView">
        <attr name="ctvText" format="string"/>
        <attr name="ctvText_size" format="dimension"/>
        <attr name="ctvText_origin_color" format="color"/>
        <attr name="ctvText_change_color" format="color"/>
        <attr name="ctvProgress" format="float"/>
        <attr name="ctvDirection" format="integer"/>
    </declare-styleable>
    <declare-styleable name="PullZoomLayout">
        <attr name="pzHeader" format="reference"/>
        <attr name="pzMain" format="reference"/>
    </declare-styleable>
    <attr name="clickableBackground" format="reference"/>
    <attr name="main_bg" format="reference|color"/>
    <attr name="white_bg" format="reference|color"/>
    <attr name="font_color" format="reference|color"/>
    <attr name="font_color2" format="reference|color"/>
    <attr name="font_red" format="reference|color"/>
    <attr name="isColorUi" format="boolean"/>
    <attr name="image_bg" format="reference|color"/>
    <attr name="divider_black" format="reference|color"/>
    <attr name="divider_light_grey" format="reference|color"/>


</resources>